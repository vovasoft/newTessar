<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="DbPlayer">
    <!-- 定义insert语句 -->


    <insert id="insert" useGeneratedKeys="false" keyProperty="id">
        insert into player (dt,newin,arrayDays,arrayWeeks,arrayMonths,arrayOthers)
        values(#{playTableDate},#{newIn},#{arrayDays},#{arrayWeeks},#{arrayMonths},#{others})
    </insert>



